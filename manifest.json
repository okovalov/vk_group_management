{
    "name": "Vk Group management",
    "description": "This extension gives you possibility to increase percentage of your friends as members of a certain group with maximum efficiency.",
    "manifest_version": 2,
    "version": "1.0.1",
    "background": {
        "scripts": [
            "background.js",
            "functions.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon" : "icon.png",
        "default_title": "Vk Group management"
    },
    "permissions": [
        "tabs",
        "storage" ,
        "http://*/*",
        "https://*/*"
    ]
}

